    "type": "outline",
    "subs": [
        {
            "text": "A game you can play with a computer and a mouse.",
            "created": "Mon, 25 Dec 2017 16:59:31 GMT",
            "flNumberedSubs": "true",
            "subs": [
                {
                    "text": "Pick any spot in the world where <a href=\"https://www.google.com/streetview/understand/\">Street View</a> has been.",
                    "created": "Mon, 25 Dec 2017 16:58:19 GMT"
                },
                {
                    "text": "Zoom out to a level where the streets are a blur of blue.",
                    "created": "Mon, 25 Dec 2017 16:58:44 GMT"
                },
                {
                    "text": "Drag the little guy into the middle of the blur.",
                    "created": "Mon, 25 Dec 2017 16:58:58 GMT"
                },
                {
                    "text": "Let up the mouse at a random spot.",
                    "created": "Mon, 25 Dec 2017 16:59:11 GMT"
                },
                {
                    "text": "See where you landed. ",
                    "created": "Mon, 25 Dec 2017 16:59:18 GMT"
                },
                {
                    "text": "Repeat.",
                    "created": "Mon, 25 Dec 2017 16:59:24 GMT"
                }
            ]
        },
        {
            "text": "PS: Hidden benefit. It'll drive the Google algorithm crazy. :rocket:",
            "created": "Mon, 25 Dec 2017 17:04:27 GMT"
        }
    ]
